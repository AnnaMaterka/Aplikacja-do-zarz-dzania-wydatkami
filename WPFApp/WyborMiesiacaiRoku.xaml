<Window x:Class="WPFApp.WyborMiesiacaiRoku"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFApp"
        mc:Ignorable="d"
        Title="WyborMiesiacaiRoku" Height="200" Width="250">
    <Grid Background="#FF1A2A41">
        <Label Content="Wybierz rok:" HorizontalAlignment="Left" Height="31" Margin="10,21,0,0" VerticalAlignment="Top" Width="140" Foreground="White"/>
        <Label Content="Wybierz miesiąc:" HorizontalAlignment="Left" Height="31" Margin="10,57,0,0" VerticalAlignment="Top" Width="112" Foreground="White"/>
        <TextBox x:Name="txtRok" HorizontalAlignment="Left" Height="31" Margin="111,21,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="79" Background="{x:Null}" BorderBrush="White" BorderThickness="0,0,0,1.5"/>
        <ComboBox x:Name="cbMiesiac" HorizontalAlignment="Left" Height="31" Margin="111,57,0,0" VerticalAlignment="Top" Width="79" BorderBrush="{x:Null}" Foreground="Black"/>
        <Button Content="Zatwierdź" HorizontalAlignment="Left" Height="31" Margin="111,103,0,0" VerticalAlignment="Top" Width="79" Click="Zatwierdz_Click" BorderBrush="{x:Null}" Background="{x:Null}" Foreground="White"/>

    </Grid>
</Window>